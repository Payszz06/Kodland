<script>
    /**
    * Генерация "уникального" clientId для Chatra
    * https://kodland.atlassian.net/browse/LX-23
    * @returns string
    */
    function generateChatraId() {
        const timestamp = Date.now()
        const suffix = Math.random().toString(36).substring(2, 15)
        
        const clientId = `${timestamp}${suffix}`
        return clientId
    }
    // Подключение Chatra

    const showChatra = typeof showEva === 'function' ? !showEva() : true

    if (showChatra) {
        (function(d, w, c) {
            w.ChatraID = 'PuBbhyP2XFd6fTH36';
            var s = d.createElement('script');
            w[c] = w[c] || function() {
                (w[c].q = w[c].q || []).push(arguments);
            };
            s.async = true;
            s.src = 'https://call.chatra.io/chatra.js';
            if (d.head) d.head.appendChild(s);
        })(document, window, 'Chatra');
    }


</script>
  
