<script>
const baseUrl = '//backoffice.kodland.org/api/v1';
var can_send = true;
function loginBlue() {
    $('#login_navbar').css('color', '#315AF2');
    $('#login_hover').show()
    $('#login_unhover').hide()
}

function loginCommon() {
    $('#login_navbar').css('color', '#000000');
