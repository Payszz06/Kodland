<script type="text/javascript">
    if (showEva()) {
        !(function () {
            function t() {
                var t = document.createElement('script');
                var h = (Math.random() * (99999 - 00001)).toFixed();
                (t.type = 'module'), (t.async = !0), (t.src = 'https://evahelper.ru/build/main.js?h=' + h), document.head.appendChild(t);
            }
