    <script>
        document.addEventListener("DOMContentLoaded", function (event) {
            const courseBalance = document.getElementById('courseBalance');
            const balanceInCourse = document.getElementById('balanceInCourse');
            const coursePayBtn = document.getElementById('coursePayBtn');

            let balance = null;
            getBalance('en')
                .then(item => {
                    balance = getBalanceValue(item);
                    let plural = getBalancePlural(item);
                    if (balance !== null) {
                        courseBalance.classList.remove('hidden');
                    }

                    if (balance == 0) {
                        courseBalance.classList.add('balance--null');
                        balanceInCourse.innerHTML = `${balance} ${plural}`;

                    } else if (balance < 0) {
                        courseBalance.classList.add('balance--negative');
                        balanceInCourse.innerHTML = `Debt ${balance} ${plural}`;
                        coursePayBtn.innerHTML = "Pay and continue the course";
                    } else {
                        balanceInCourse.innerHTML = `${balance} ${plural}`;
                    }
                })

                // <hint-provider />
                new Vue({
                    el: '#hint-provider-vue',
                    delimiters: ['[[', ']]'],
                })
        });

      function csrfSafeMethod(method) {
        return (/^(GET|HEAD|OPTIONS|TRACE)$/.test(method));
      }

      $(document).ready(function(e) {
        var csrftoken = getCookie('csrftoken');
        $.ajaxSetup({
          beforeSend: function(xhr, settings) {
          if (!csrfSafeMethod(settings.type) && !this.crossDomain) {
            xhr.setRequestHeader("X-CSRFToken", csrftoken);
          }
        }
        });
      });
      function getPaymentOrder() {
       $.ajax({
           url: '//platform.kodland.org/en/simple_pay_button/',
           type: "GET",
           headers: {
               'Authorization': bearer
           },
           success: function (result, data) {
               window.open(result);
           },
           error: function () {
               M.toast({html: "Oops, something went wrong :(", displayLength: 3000});
           }
        });
      }
      $('#coursePayBtn').on('click', function () {
          getPaymentOrder();
      });

        function getSimplePayBanner() {
            const getCookie = function (name) {
                let matches = document.cookie.match(new RegExp(
                    "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
                ));
                const cookie = matches ? decodeURIComponent(matches[1]) : undefined;
                if (name === 'access') {
                    const login_url = location.protocol + '//' + location.hostname + '/login/?next=' + location.pathname;
                    return cookie ? cookie : window.location.replace(login_url);
                }
                return cookie;
            }

            $.ajax({
                url: '//backoffice.kodland.org/api/v1/billing/check-banner-condition/?student_id=1120163',
                type: "GET",
                headers: {
                    'Authorization': `Bearer ${getCookie('access')}`
                },
                success: function (result) {
                    const {
                        answer,
                        banner_text,
                        regional_discount,
                        enable_at,
                        enable_end,
                        text_day_for_banner,
                        text_hour_for_banner,
                        text_minute_for_banner,
                        text_second_for_banner,
                        text_above_timer_for_banner,
                    } = result
                    if (!answer || new Date(enable_at) > new Date()) return
                    $.ajax({
                        url: '//backoffice.kodland.org/api/v1/simple-pay-banner/generate/?student_id=1120163',
                        type: "GET",
                        headers: {
                            'Authorization': `Bearer ${getCookie('access')}`
                        },
                        success: function (result) {
                            const { url } = result

                            showBanner(url, banner_text, regional_discount, enable_end, {
                                day: text_day_for_banner,
                                hour: text_hour_for_banner,
                                minute: text_minute_for_banner,
                                second: text_second_for_banner,
                                button: text_above_timer_for_banner,
                            })
                        },
                    });
                },
            });
        }

        class CountdownTimer {
            constructor(deadline, cbChange, cbComplete) {
                this._deadline = deadline;
                this._cbChange = cbChange;
                this._cbComplete = cbComplete;
                this._timerId = null;
                this._out = {
                    days: '', hours: '', minutes: '', seconds: '',
                    daysTitle: '', hoursTitle: '', minutesTitle: '', secondsTitle: ''
                };
                this._start();
            }
            static declensionNum(num, words) {
                return words[(num % 100 > 4 && num % 100 < 20) ? 2 : [2, 0, 1, 1, 1, 2][(num % 10 < 5) ? num % 10 : 5]];
            }
            _start() {
                this._calc();
                this._timerId = setInterval(this._calc.bind(this), 1000);
            }
            _calc() {
                const diff = this._deadline - new Date();
                const days = diff > 0 ? Math.floor(diff / 1000 / 60 / 60 / 24) : 0;
                const hours = diff > 0 ? Math.floor(diff / 1000 / 60 / 60) % 24 : 0;
                const minutes = diff > 0 ? Math.floor(diff / 1000 / 60) % 60 : 0;
                const seconds = diff > 0 ? Math.floor(diff / 1000) % 60 : 0;
                this._out.days = days < 10 ? '0' + days : days;
                this._out.hours = hours < 10 ? '0' + hours : hours;
                this._out.minutes = minutes < 10 ? '0' + minutes : minutes;
                this._out.seconds = seconds < 10 ? '0' + seconds : seconds;
                // this._out.daysTitle = CountdownTimer.declensionNum(days, ['день', 'дня', 'дней']);
                // this._out.hoursTitle = CountdownTimer.declensionNum(hours, ['час', 'часа', 'часов']);
                // this._out.minutesTitle = CountdownTimer.declensionNum(minutes, ['минута', 'минуты', 'минут']);
                // this._out.secondsTitle = CountdownTimer.declensionNum(seconds, ['секунда', 'секунды', 'секунд']);


                this._cbChange ? this._cbChange(this._out) : null;
                if (diff <= 0) {
                    clearInterval(this._timerId);
                    this._cbComplete ? this._cbComplete() : null;
                }
            }
        }

        function showBanner(bannerUrl, bannerText, discount, enableEnd, labels = {}) {
            document.getElementById('simplePayBanner').classList.remove('d-none')
            document.getElementById('simplePayBannerLink').href = bannerUrl
            document.getElementById('simplePayBannerLink').innerHTML = labels.button || '&#60;get&#62;'

            const discountIndex1 = bannerText.indexOf('[discount]')
            const discountIndex2 = bannerText.indexOf('{discount}')
            if (discountIndex1 > -1) {
                document.getElementById('simplePayBannerText').innerHTML =
                    bannerText.substring(0, discountIndex1) +
                    `${discount}` +
                    bannerText.substring(discountIndex1 + 10, bannerText.length)
            } else if(discountIndex2 > -1) {
                document.getElementById('simplePayBannerText').innerHTML =
                    bannerText.substring(0, discountIndex2) +
                    `${discount}` +
                    bannerText.substring(discountIndex2 + 10, bannerText.length)
            } else {
                document.getElementById('simplePayBannerText').innerHTML = bannerText
            }

            const elDays1 = document.querySelector('.timer-1 .timer__days');
            const elHours1 = document.querySelector('.timer-1 .timer__hours');
            const elMinutes1 = document.querySelector('.timer-1 .timer__minutes');
            const elSeconds1 = document.querySelector('.timer-1 .timer__seconds');

            elDays1.dataset.title = labels.day;
            elHours1.dataset.title = labels.hour;
            elMinutes1.dataset.title = labels.minute;
            elSeconds1.dataset.title = labels.second;

            const deadline = new Date(enableEnd);
            new CountdownTimer(deadline, (timer) => {
                elDays1.innerHTML = `<span>${String(timer.days)[0]}</span> <span>${String(timer.days)[1]}</span>`;
                elHours1.innerHTML = `<span>${String(timer.hours)[0]}</span> <span>${String(timer.hours)[1]}</span>`;
                elMinutes1.innerHTML = `<span>${String(timer.minutes)[0]}</span> <span>${String(timer.minutes)[1]}</span>`;
                elSeconds1.innerHTML = `<span>${String(timer.seconds)[0]}</span> <span>${String(timer.seconds)[1]}</span>`;
            }, () => {
                hideBanner()
            });
            if (window.ym) window.ym(87480480, 'reachGoal', 'repeated_sale_banner_discount_show')
        }

        function hideBanner() {
            document.getElementById('simplePayBanner').classList.add('d-none')
        }


        getSimplePayBanner()
        function bannerLinkClicked() {
            if (window.ym) window.ym(87480480, 'reachGoal', 'repeated_sale_banner_discount_click')
        }
    </script>

