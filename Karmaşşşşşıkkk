function show_not_circ(data){
    total_notificats = data
    if (data <10){
    width = '15px'
    }
    if (data>9){
    width = '23px'
    }
    if (data>99){
    data = '99+';
    width = '31px';
    }
    $(".message_circle_inner").text(data);
    $(".message_circle").show();
    $(".message_circle").animate({
    height: "15px",
    width: width,
    marginTop: "-22px",
    marginLeft: "15px"
    },300);
    $(".message_circle_inner").animate({
    fontSize: "14px"
    },300);
}
